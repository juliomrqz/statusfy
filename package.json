{
  "name": "statusfy",
  "version": "0.0.1a",
  "description": "A marvelous open source Status Page system",
  "author": "Julio Marquez",
  "main": "lib/index.js",
  "bin": {
    "statusfy": "bin/statusfy.js"
  },
  "scripts": {
    "dev": "cd demo && npm run dev",
    "build": "cd demo && npm run build",
    "start": "cd demo && npm run start",
    "generate": "cd demo && npm run generate",
    "new-incident": "cd demo && npm run new-incident",
    "lint": "npx eslint --fix --ext .js,.vue --ignore-path .gitignore . bin/ lib/ client/ test/",
    "lint:css": "stylelint 'client/**/*.{css,vue}'",
    "precommit": "npm run lint && npm run lint:css",
    "test": "cross-env NODE_ENV=test node test/prepare.js && jest --config test/jest.config.js",
    "svg": "vsvg -s ./client/assets/icons -t ./client/components/icons --es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bazzite/statusfy.git"
  },
  "bugs": {
    "url": "https://github.com/bazzite/statusfy/issues"
  },
  "homepage": "https://statusfy.co",
